<h1>New Attraction</h1>
<%= form_for @attraction do |f| %>
  Name: <%= f.text_field :name %> </br>
  Tickets: <%= f.number_field :tickets %> </br>
  Min Height: <%= f.number_field :min_height %> </br>
  Nausea Rating: <%= f.number_field :nausea_rating %> </br>
  Happiness Rating: <%= f.number_field :happiness_rating %> </br>
  <%= f.submit "Create Attraction" %>
<% end %>
