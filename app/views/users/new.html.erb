<h1>Create Your Account!!</h1>

<%=form_for(@user) do |form|%>
  <%=form.label :name%>
  <%=form.text_field :name%><br>
  <%=form.label :height%>
  <%=form.text_field :height%><br>
  <%=form.label :happiness%>
  <%=form.text_field :happiness%><br>
  <%=form.label :nausea%>
  <%=form.text_field :nausea%><br>
  <%=form.label :tickets%>
  <%=form.text_field :tickets%><br>
  <%=form.label :password%>
  <%=form.password_field :password%><br>
  <%=form.label :admin%>
  <%=form.check_box :admin%><br>
  <%=form.submit%>
<%end%>